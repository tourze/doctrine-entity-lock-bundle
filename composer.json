{
  "name": "tourze/doctrine-entity-lock-bundle",
  "description": "为 Doctrine 实体提供自动锁定和刷新机制",
  "license": "MIT",
  "require": {
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/lock": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/lock-service-bundle": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/biz-user-bundle": "0.0.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineEntityLockBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineEntityLockBundle\\Tests\\": "tests"
    }
  }
}