{
  "name": "tourze/doctrine-entity-lock-bundle",
  "description": "实体加锁",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/orm": "^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/doctrine-bridge": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/lock": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/lock-service-bundle": "0.1.*",
    "tourze/symfony-integration-test-kernel": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineEntityLockBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineEntityLockBundle\\Tests\\": "tests"
    }
  }
}