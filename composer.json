{
  "name": "tourze/doctrine-entity-lock-bundle",
  "description": "实体加锁",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^2.20 || ^3.0",
    "symfony/framework-bundle": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/lock": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/lock-service-bundle": "0.0.*",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineEntityLockBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineEntityLockBundle\\Tests\\": "tests"
    }
  }
}